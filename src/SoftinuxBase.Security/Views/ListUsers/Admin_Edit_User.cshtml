@{
    Layout = null;
}

@model SoftinuxBase.Security.Data.Entities.User

<div class="form-group">
    <label for="userName" class="col-sm-2 control-label">User Name</label>
    <div class="col-sm-10">
        <input id="userName" class="form-control admin-label" readonly>
    </div>
</div>
<div class="form-group">
    <label for="lastConnexion" class="col-sm-2 control-label">Last Connexion</label>
    <div class="col-sm-10">
        <input id="lastConnexion" class="form-control admin-label" readonly>
    </div>
</div>
<div class="form-group">
    <label for="emailconfirmed" class="col-sm-2 control-label">Email Confirmed</label>
    <div class="col-sm-10">
        <input type="checkbox" id="emailconfirmed" name="iCheck">
    </div>
</div>
<div class="form-group">
    <label for="twofactorauth" class="col-sm-2 control-label">Two Factor Authentication</label>
    <div class="col-sm-10">
        <input type="checkbox" id="twofactorauth" name="iCheck">
    </div>
</div>

@Html.Partial("_User_Profile_Details_Fields")
