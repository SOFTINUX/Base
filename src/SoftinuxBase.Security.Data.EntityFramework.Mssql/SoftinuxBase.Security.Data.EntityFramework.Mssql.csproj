<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>Benjamin Nolmans, Barbara Post</Authors>
    <Company>SOFTINUX</Company>
    <Copyright>Copyright © 2017 Softinux</Copyright>
    <Version>0.0.1</Version>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>SoftinuxBase.Security.Data.EntityFramework.Sqlite</AssemblyName>
    <RootNamespace>SoftinuxBase.Security.Data.EntityFramework.Sqlite</RootNamespace>
    <PackageId>SoftinuxBase.Security.Data.EntityFramework.Sqlite</PackageId>
    <PackageIconUrl></PackageIconUrl>
    <PackageProjectUrl>http://www.softinux.com/</PackageProjectUrl>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>8</LangVersion>
    <CodeAnalysisRuleSet>..\..\StyleCopRules.ruleset</CodeAnalysisRuleSet>
    <RepositoryUrl>https://github.com/SOFTINUX/Base</RepositoryUrl>
  </PropertyGroup>
  <ImportGroup Label="Documentation">
    <Import Project="..\..\documentation.props" />
  </ImportGroup>
  <ItemGroup>
    <AdditionalFiles Include="..\..\stylecop.json" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SoftinuxBase.Security.Data.Abstractions\SoftinuxBase.Security.Data.Abstractions.csproj" />
    <ProjectReference Include="..\SoftinuxBase.Security.Data.EntityFramework\SoftinuxBase.Security.Data.EntityFramework.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="3.1.1" />
    <PackageReference Include="StyleCop.Analyzers" Version="1.1.118">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
