<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Authors>Benjamin Nolmans, Barbara Post</Authors>
    <Company>SOFTINUX</Company>
    <Copyright>Copyright © 2017 Softinux</Copyright>
    <Version>0.0.1</Version>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AssemblyName>SoftinuxBase.Security.Data.EntityFramework</AssemblyName>
    <RootNamespace>SoftinuxBase.Security.Data.EntityFramework</RootNamespace>
    <PackageId>SoftinuxBase.Security.Data.EntityFramework</PackageId>
    <PackageIconUrl></PackageIconUrl>
    <PackageProjectUrl>http://www.softinux.com/</PackageProjectUrl>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <LangVersion>7</LangVersion>
    <CodeAnalysisRuleSet>$(SolutionDir)StyleCopRules.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <ItemGroup>
    <Content Remove="$(SolutionDir)stylecop.json" />
  </ItemGroup>
  <ItemGroup>
    <AdditionalFiles Include="$(SolutionDir)stylecop.json" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="ExtCore.Data.EntityFramework" Version="4.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="2.2.0" />
    <PackageReference Include="StyleCop.Analyzers" Version="1.0.2">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SoftinuxBase.Security.Common\SoftinuxBase.Security.Common.csproj" />
    <ProjectReference Include="..\SoftinuxBase.Security.Data.Abstractions\SoftinuxBase.Security.Data.Abstractions.csproj" />
  </ItemGroup>
</Project>